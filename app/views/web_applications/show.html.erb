<div class="status">
  <h2><%= @web_application.name %></h2>
  <b>Status:</b>&nbsp;
  <% if @web_app_available %>
    <b class="text-success">AVAILABLE</b>
  <% else %>
    <b class="text-alert">DOWN</b>
  <% end %>
</div>
<br>
<div>
  <%= link_to "edit", edit_web_application_path(@web_application) %>
</div>