<h2>Change <%= @web_application.name%></h2>
<%= render partial: "form", locals: {current_user: @current_user, web_application: @web_application} %>
<br>
<%= form_for @web_application, html: { method: :delete, id: "web-app-delete" } do |f| %>
  <%= f.submit "Delete this Web application", confirm: "Are you sure you want to delete this application? This will permanently remove all application data." %>
<% end %>